{
  "name": "amazon-order-reports-api",
  "version": "3.3.2",
  "description": "Scrapes orders and refunds from Amazon.com",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc -b src",
    "clean": "rm -rf buildcache coverage lib test-output",
    "commit": "cz",
    "coverage": "jest --coverage --no-cache",
    "docs": "typedoc",
    "lint": "eslint src",
    "prepack": "npm run clean && npm run build",
    "start": "node lib/index.js",
    "start:dev": "ts-node --dir src index.ts",
    "test": "jest",
    "test:ci": "jest --ci"
  },
  "author": "Shaun Starsprung <s@starsprung.com>",
  "repository": "github:starsprung/amazon-order-reports-api",
  "license": "MIT",
  "dependencies": {
    "camelcase": "^5.3.1",
    "csv-parse": "^4.14.2",
    "delay": "^4.4.0",
    "luxon": "^1.25.0",
    "otplib": "^12.0.1",
    "puppeteer": "^5.5.0",
    "puppeteer-extra": "^3.1.15",
    "puppeteer-extra-plugin-stealth": "^2.6.5",
    "query-string": "^6.13.7",
    "uuid": "^3.4.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/github": "^7.2.0",
    "@semantic-release/npm": "^7.0.9",
    "@semantic-release/release-notes-generator": "^9.0.1",
    "@types/app-root-path": "^1.2.4",
    "@types/luxon": "^1.25.0",
    "@types/mockdate": "^2.0.0",
    "@types/node": "^14.14.16",
    "@types/puppeteer": "^5.4.2",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.11.0",
    "@typescript-eslint/parser": "^4.11.0",
    "app-root-path": "^3.0.0",
    "commitizen": "^4.2.2",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.16.0",
    "husky": "^4.3.6",
    "jest": "^26.6.3",
    "jest-junit": "^12.0.0",
    "mockdate": "^3.0.2",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "semantic-release": "^17.3.0",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "typedoc": "^0.20.6",
    "typescript": "^4.1.3"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "pretty-quick --staged"
    }
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git",
      "@semantic-release/github"
    ]
  }
}
